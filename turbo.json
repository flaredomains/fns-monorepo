{
    "$schema": "https://turbo.build/schema.json",
    "pipeline": {
      "build": {
        "dependsOn": ["^build"],
        "outputs": ["dist/**", "out/**"]
      },
      "test": {
        "dependsOn": ["build"],
        "inputs": ["src/**/*.tsx", "src/**/*.ts", "test/**/*.ts", "test/**/*.tsx", "test/**/*.t.sol"]
      },
      "lint": {
        "outputs": []
      },
      "deploy": {
        "dependsOn": ["build", "test", "lint"]
      },
      "dev": {
        "cache": false
      },
      "clean": {}
    }
  }